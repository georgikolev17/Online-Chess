using System;

namespace Online_Chess
{
    class Program
    {
        static void Main(string[] args)
        {
            string startPosition = Console.ReadLine();
            string finalPosition = Console.ReadLine();

            int startRow = int.Parse(startPosition[1].ToString());
            int startCol = (int)startPosition[0]-97;
            int finalRow = (int)finalPosition[1] - 97;
            int finalCol = 7-int.Parse((finalPosition[0]).ToString());

            char[,] chessBoard = new char[8, 8]
            {
                {'R' , 'K', 'B', 'K', 'Q', 'B', 'K', 'R'},
                {'P' , 'P', 'P', 'P', 'P', 'P', 'P', 'P'},
                {' ' , ' ', ' ', ' ', ' ', ' ', ' ', ' '},
                {' ' , ' ', ' ', ' ', ' ', ' ', ' ', ' '},
                {' ' , ' ', ' ', ' ', ' ', ' ', ' ', ' '}, 
                {' ' , ' ', ' ', ' ', ' ', ' ', ' ', ' '}, 
                {'p' , 'p', 'p', 'p', 'p', 'p', 'p', 'p'},
                {'r' , 'k', 'b', 'k', 'q', 'b', 'k', 'r'}
            };

            Console.WriteLine(chessBoard[startRow,startCol]);
            Console.WriteLine(chessBoard[finalRow, finalCol]);
        }
    }
}
